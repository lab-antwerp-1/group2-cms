{
  "name": "",
  "private": true,
  "version": "0.1.0",
  "description": "A Strapi application",
  "scripts": {
    "develop": "strapi develop",
    "start": "strapi start",
    "build": "strapi build",
    "strapi": "strapi",
    "test": "node --experimental-vm-modules node_modules/.bin/jest --silent --passWithNoTests --testPathIgnorePatterns=lib ./public",
    "format": "prettier --write ./",
    "format:check": "prettier --check ./public",
    "spell-check": "cspell  './public/**'",
    "lint:ls": "ls-lint ./public",
    "lint:md": "markdownlint --ignore node_modules --config ./.markdownlint.jsonc --fix ./",
    "lint:css": "stylelint --fix './public/**/*.css' ",
    "lint:js": "eslint --max-warnings=0 --ext=.js --format=codeframe ./public/src",
    "validate:html": "html-validate ./public",
    "accessibility": "achecker ./public"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "accessibility-checker": "^3.1.25",
    "cspell": "^5.6.6",
    "dependency-cruiser": "^10.0.1",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-boundaries": "^2.3.0",
    "eslint-plugin-disable": "^2.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.3.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-sonarjs": "^0.9.1",
    "graphviz-cli": "^1.0.0",
    "html-validate": "^5.2.0",
    "jest": "^26.6.3",
    "jsdoc-to-markdown": "^6.0.1",
    "markdownlint-cli": "^0.28.1",
    "prettier": "^2.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0"
  },
  "dependencies": {
    "@ls-lint/ls-lint": "^1.11.0",
    "@strapi/plugin-i18n": "4.1.7",
    "@strapi/plugin-users-permissions": "4.1.7",
    "@strapi/strapi": "4.1.7",
    "better-sqlite3": "7.4.6"
  },
  "author": {
    "name": "A Strapi developer"
  },
  "strapi": {
    "uuid": "aa3194df-ba27-4922-b365-d0a1e92b79a5"
  },
  "engines": {
    "node": ">=12.x.x <=16.x.x",
    "npm": ">=6.0.0"
